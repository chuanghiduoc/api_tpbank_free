# TPBank credentials (TPBANK_ prefix to avoid Windows env var conflicts)
TPBANK_USERNAME=your_tpbank_username
TPBANK_PASSWORD=your_tpbank_password
DEVICE_ID=your_device_id
ACCOUNT_ID=your_account_id

# Transaction history settings
DAYS=30

# Server settings (optional)
PORT=3000

# Proxy settings (optional - leave empty if not using proxy)
PROXY_SCHEMA=http
PROXY_IP=
PROXY_PORT=
PROXY_USERNAME=
PROXY_PASSWORD=